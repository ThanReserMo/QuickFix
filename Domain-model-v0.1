@startuml
class Customer {
  +name: String
  +email: String
  +location: String
}

class Technician {
  +name: String
  +skills: String
  +ratings: Float
}

class Service {
  +type: String
  +price: Float
}

class Payment {
  +amount: Float
  +method: String
}

class Review {
  +rating: Integer
  +comment: String
}

class Administrator {
  +manageCustomers()
  +manageTechnicians()
}

Customer "1" -- "*" Service : requests
Service "1" -- "1" Technician : providedBy
Customer "1" -- "*" Payment : makes
Customer "1" -- "*" Review : writes
Review "1" -- "1" Technician : for
Administrator "1" -- "*" Customer : manages
Administrator "1" -- "*" Technician : manages
@enduml
